[pixel shader]
typedef float2 Dword;
typedef float3 dWord;

float4 f()
{
    typedef Dword dword;
    dword v1 = {1, 2};
    DWORD v4 = 4;
    return float4(v1.x, v1.y, 3, v4);
}

float4 f2()
{
    typedef dword dword;
    dword v = 1;
    return float4(v, v, v, v);
}

float4 main() : SV_TARGET
{
    return f() + f2();
}

